<h3> <%= t('.title', question: @question.body) %> </h3>

<%= form_with model: [:admin, @test, @question], local: true do |form| %>
  <%= render "shared/errors", resouse: @question %>

  <p>
    <%= form.label :body, t('.body') %><br>
    <%= form.text_field :body %>
  </p>

  <%= form.submit %>
<% end %>

<br>
